
<div class="col-md-4">
	<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	  <fieldset>
	  	<legend>Sign In</legend>
		  <%= f.input :email, autofocus: true, wrapper_html: { class: "form-group" }, input_html: { class: "form-control" } %>

		  <%= f.input :password, autocomplete: "off", wrapper_html: { class: "form-group" }, input_html: { class: "form-control" } %>

		  <% if devise_mapping.rememberable? -%>
		    <%= f.input :remember_me, as: :boolean %>
		  <% end -%>

		  <div><%= f.button :submit, "Sign in" %></div>
		</fieldset>
	<% end %>

	<%= render "devise/shared/links" %>
</div>
<div class="col-md-8">
	<h2>Welcome to Treebook</h2>
</div>

